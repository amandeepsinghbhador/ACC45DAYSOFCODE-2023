include <iostream>
using namespace std;

int main() {
   int t;
   cin>>t;
   for(int i=1;i<=t;i++)
   {
    const int X=5; //X has contain 5 rs coin
    const int Y=10; // Y has contain 10 rs coin
     int x,y,z;
      cin>>x>>y>>z;
      int total;
      total=x*X+y*Y;
      cout<<total/z<<endl;
   }
	return 0;
}
